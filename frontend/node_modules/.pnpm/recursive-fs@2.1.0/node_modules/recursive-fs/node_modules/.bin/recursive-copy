#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pablo/Downloads/MedSynapse/frontend/node_modules/.pnpm/recursive-fs@2.1.0/node_modules/recursive-fs/bin/node_modules:/Users/pablo/Downloads/MedSynapse/frontend/node_modules/.pnpm/recursive-fs@2.1.0/node_modules/recursive-fs/node_modules:/Users/pablo/Downloads/MedSynapse/frontend/node_modules/.pnpm/recursive-fs@2.1.0/node_modules:/Users/pablo/Downloads/MedSynapse/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pablo/Downloads/MedSynapse/frontend/node_modules/.pnpm/recursive-fs@2.1.0/node_modules/recursive-fs/bin/node_modules:/Users/pablo/Downloads/MedSynapse/frontend/node_modules/.pnpm/recursive-fs@2.1.0/node_modules/recursive-fs/node_modules:/Users/pablo/Downloads/MedSynapse/frontend/node_modules/.pnpm/recursive-fs@2.1.0/node_modules:/Users/pablo/Downloads/MedSynapse/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/recursive-copy" "$@"
else
  exec node  "$basedir/../../bin/recursive-copy" "$@"
fi
