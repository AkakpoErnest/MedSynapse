# Environment variables for MedSynapse

# Server Configuration
PORT=3001
NODE_ENV=development

# Blockchain Configuration
MUMBAI_RPC_URL=https://rpc-mumbai.maticvigil.com
PRIVATE_KEY=your_private_key_here
CONTRACT_ADDRESS=your_contract_address_here

# Lighthouse Configuration
LIGHTHOUSE_API_KEY=your_lighthouse_api_key_here
LIGHTHOUSE_GATEWAY_URL=https://gateway.lighthouse.storage

# Envio HyperSync Configuration
ENVIO_API_KEY=your_envio_api_key_here
ENVIO_ENDPOINT=https://api.envio.dev

# JWT Secret
JWT_SECRET=your_jwt_secret_here

# Database (if needed later)
DATABASE_URL=postgresql://user:password@localhost:5432/medsynapse
